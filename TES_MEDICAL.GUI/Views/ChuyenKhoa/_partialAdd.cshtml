    {
        @Html.AntiForgeryToken()


        <div class="text-danger"></div>


        <div class="form-group col-12">
            @Html.LabelFor(model => model.TenCK, "Tên chuyên khoa", htmlAttributes: new { @class = "control-label col-md-6" })
