<body>
    <!-- Login Full Background -->
    <!-- For best results use an image with a resolution of 1280x1280 pixels (prefer a blurred image for smaller file size) -->
    <img src="~/MainElement/img/background.jpg" alt="Login Full Background" class="full-bg animation-pulseSlow">
    <!-- END Login Full Background -->
    <!-- Login Container -->
    <div id="login-container" class="animation-fadeIn" style="margin-top: -100px;">
        <!-- Login Title -->
        <div class="login-title text-center">
            <h1><strong>TES MEDICAL</strong><br></h1>
            <!-- <h1><strong>TES MEDICAL</strong><br><small>Please <strong>Login</strong> or <strong>Register</strong></small></h1> -->
        </div>
        <!-- END Login Title -->
        <!-- Login Block -->
        <div class="block push-bit">
            <!-- Login Form -->
            <form action="index.html" method="post" id="form-login" class="form-horizontal form-bordered form-control-borderless">
                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" style="color: rgb(54, 172, 105);"><i class="fad fa-envelope-open-text"></i></span>
                            <input type="text" id="login-email" name="login-email" class="form-control input-lg" placeholder="Email">
                        </div>
                    </div>
                </div>
                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" style="color: rgb(54, 172, 105);"><i class="fad fa-lock-alt"></i></span>
                            <input type="password" id="login-password" name="login-password" class="form-control input-lg" placeholder="Mật khẩu">
                        </div>
                    </div>
                </div>
                <div class="form-group form-actions">
                    <div class="col-xs-6">
                        <div class="row">
                            <label class="col-sm-4" data-toggle="tooltip" title="Remember Me?">
                                <input type="checkbox" id="login-remember-me" name="login-remember-me" checked>
                                <span></span>
                            </label>
                            <p class="col-sm-8" style="margin-left: -50px; color: rgb(54, 172, 105);">Ghi nhớ đăng nhập</p>
                        </div>
                    </div>
                    <div class="col-xs-6 text-right">
                        <a href="javascript:void(0)" id="link-reminder-login" style="color: rgb(54, 172, 105);">Quên mật khẩu ?</a>
                    </div>

                </div>
                <div class="form-group">
                    <div class="col-xs-12 text-center" style="margin-bottom: 10px;">
                        <button type="submit" class="btn" style="width: 250px; background-color: rgb(54, 172, 105); color: white;">Đăng nhập</button>
                    </div>

                    <div class="form-google col-xs-12 text-center">
                        <a href="" class="btn" style="border: 1px solid gray; border-radius: 5px; width: 250px; color: black">
                            <img style="margin-right: 20px;" src="~/MainElement/img/google.jpg" width="20px;" alt="">  Đăng nhập với Google
                        </a>
                    </div>
                </div>
            </form>
            <!-- END Login Form -->
            <!-- Reminder Form -->
            <form action="login_full.html#reminder" method="post" id="form-reminder" class="form-horizontal form-bordered form-control-borderless display-none">
                <div class="form-group">
                    <div class="col-xs-12">
                        <div class="input-group">
                            <span class="input-group-addon" style="color: rgb(54, 172, 105);"><i class="fad fa-envelope-open-text"></i></span>                                <input type="text" id="reminder-email" name="reminder-email" class="form-control input-lg" placeholder="Email">
                        </div>
                    </div>
                </div>
                <div class="form-group form-actions">
                    <div class="col-xs-4">
                        <a href="javascript:void(0)" id="link-reminder" style="color: rgb(54, 172, 105);">Đăng nhập lại</a>
                    </div>
                    <div class="col-xs-8 text-right">
                        <button type="submit" class="btn" style="width: 150px; background-color: rgb(54, 172, 105); color: white;"><i class="fad fa-exchange"></i> Xác nhận</button>
                    </div>
                </div>
            </form>
            <!-- END Reminder Form -->
        </div>
        <!-- END Login Block -->
    </div>
    <!-- END Login Container -->
