

@model TES_MEDICAL.GUI.Models.TheLoai

<h4>Cập nhật thông tin</h4>
<hr />
<div>
    @using (Html.BeginForm(null, null, FormMethod.Get, new { @class = "d-flex flex-wrap", name = "frm", id = "frm", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()


        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="form-group col-4" style="display:none">
            <label class="control-label col-md-6" for="MaTL">MaTL</label>
            <div class="col-md-10">
                @Html.EditorFor(model => model.MaTL, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.MaTL, "", new { @class = "text-danger" })
            </div>
        </div>


        <div class="form-group col-4">
            @Html.LabelFor(model => model.TenTL, "Tên thể loại", htmlAttributes: new { @class = "control-label col-md-6" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.TenTL, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.TenTL, "", new { @class = "text-danger" })
            </div>
        </div>







    }



</div>
@{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }

