

@model TES_MEDICAL.GUI.Models.TheLoai

<h4 style="text-align:center; font-weight:bold">THÊM THỂ LOẠI</h4>
<hr />
<div>
    @using (Html.BeginForm(null, null, FormMethod.Get, new { @class = "d-flex flex-wrap", name = "frm", id = "frm", enctype = "multipart/form-data" }))
    {
        @Html.AntiForgeryToken()

        <div class="text-danger"></div>


        <div class="form-group col-4">
            @Html.LabelFor(model => model.TenTL, "Tên thể loại", htmlAttributes: new { @class = "control-label col-md-12" })
            <div class="col-md-10">
                @Html.EditorFor(model => model.TenTL, new { htmlAttributes = new { @class = "form-control" } })
                @Html.ValidationMessageFor(model => model.TenTL, "", new { @class = "text-danger" })
            </div>
        </div>






    }





</div>

@{ await Html.RenderPartialAsync("_ValidationScriptsPartial"); }


